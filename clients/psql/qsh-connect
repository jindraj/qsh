#!/bin/bash

QSH_SCRIPTS=$QSH_BASE/scripts
QSH_CLIENTS=$QSH_BASE/clients

if [[ -f "$CONNECTION_FILE" ]]; then
  eval "PGPASSFILE='$CLIENT_CONNECTION_FILE' QSH_CONNECTION=\"$CONNECTION\" $CLIENT $INVOCATION $PAGER_ALWAYS $QSH_PROMPT";
else
  eval "QSH_INVOCATION=\"$INVOCATION\" $CLIENT $INVOCATION $PAGER_ALWAYS $QSH_PROMPT";
fi;

