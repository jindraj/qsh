#!/bin/bash

QSH_TMP=/tmp/qsh

CLIENT_PANE_ID=$1
QUERY_FILE=$2

RESULT_REQUEST="$QSH_TMP/client.${CLIENT_PANE_ID}.result-request";
RESULT_REQUEST_COMPLETE="${RESULT_REQUEST}.complete";

echo >> "$QUERY_FILE"
echo "\\> $RESULT_REQUEST_COMPLETE" >> "$QUERY_FILE"
echo "\\>" >> "$QUERY_FILE"
echo >> "$QUERY_FILE

